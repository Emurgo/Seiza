{
  "devDependencies": {
    "eslint": "^5.14.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "prettier-eslint-cli": "^4.7.1"
  },
  "lint-staged": {
    "{graphql-server,frontend}/src/**/*.js": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
