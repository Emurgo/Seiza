{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development node server.js",
    "build": "next build ./src",
    "build:local": "REACT_APP_SENTRY_RELEASE_VERSION=`sentry-cli releases propose-version` yarn build",
    "build:heroku": "sentry-cli releases new $SOURCE_VERSION; REACT_APP_SENTRY_RELEASE_VERSION=$SOURCE_VERSION yarn build; sentry-cli releases files $SOURCE_VERSION upload-sourcemaps --ext map ./build",
    "start": "if test ! -z $(echo $NODE_HOME | grep heroku); then yarn start:heroku; else yarn start:local; fi",
    "start:local": "REACT_APP_SENTRY_RELEASE_VERSION=`sentry-cli releases propose-version` NODE_ENV=production node server.js",
    "start:heroku": "REACT_APP_SENTRY_RELEASE_VERSION=$HEROKU_SLUG_COMMIT NODE_ENV=production node server.js",
    "heroku-postbuild": "yarn build:heroku",
    "eslint": "eslint src --max-warnings=0",
    "flow": "flow",
    "flow-start": "flow start",
    "flow-stop": "flow stop",
    "flow-coverage": "flow-coverage-report || exit 0",
    "prettier": "prettier-eslint --write \"src/**/*.js\"",
    "extract-translations": "NODE_ENV=development src/i18n/scripts/cli.js --output src/i18n/locales/ --locales en,ja-JP,es-ES,ru-RU  'src/**/*.js' --flat",
    "test": "yarn flow && yarn eslint",
    "update-schema-type": "yarn gql-gen"
  },
  "dependencies": {
    "@material-ui/core": "^4.0.1",
    "@material-ui/icons": "^4.0.1",
    "@material-ui/styles": "^4.0.1",
    "@nivo/line": "^0.58.0",
    "@react-google-maps/api": "^1.5.1",
    "@sentry/browser": "^5.4.0",
    "@sentry/integrations": "^5.4.2",
    "@sentry/node": "^5.4.2",
    "@sentry/webpack-plugin": "^1.6.2",
    "@svgr/webpack": "^4.3.0",
    "@welldone-software/why-did-you-render": "^3.2.1",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "apollo-cache-inmemory": "^1.6.0",
    "apollo-client": "^2.6.0",
    "apollo-link-http": "^1.5.14",
    "bignumber.js": "^9.0.0",
    "bowser": "^2.5.2",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "copy-to-clipboard": "^3.2.0",
    "crc-32": "^1.2.0",
    "css-mediaquery": "^0.1.2",
    "customize-cra": "^0.2.12",
    "dayjs-ext": "^2.2.0",
    "dotenv-webpack": "^1.7.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "express": "^4.17.1",
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.1",
    "idx": "^2.5.6",
    "isemail": "^3.2.0",
    "isomorphic-unfetch": "^3.0.0",
    "jdenticon": "^2.1.1",
    "load-google-maps-api": "^2.0.1",
    "lru-cache": "^5.1.1",
    "next": "^11.1.3",
    "next-compose-plugins": "^2.2.0",
    "next-transpile-modules": "^2.3.1",
    "nookies": "^2.0.6",
    "qrcode.react": "^0.9.2",
    "query-string": "^6.5.0",
    "rc-slider": "^8.6.11",
    "react": "^16.8.6",
    "react-addons-css-transition-group": "^15.6.2",
    "react-apollo": "^2.5.6",
    "react-apollo-hooks": "^0.4.5",
    "react-dom": "^16.8.6",
    "react-intl": "^2.9.0",
    "react-markdown": "^4.0.8",
    "react-measure": "^2.3.0",
    "react-no-ssr": "^1.1.0",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1",
    "recharts": "^1.6.2",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "smoothscroll-polyfill": "^0.4.4",
    "unfetch": "^4.1.0",
    "url-loader": "^2.0.0",
    "use-react-router": "^1.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@next/bundle-analyzer": "^9.0.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-react-intl-auto": "^1.7.0",
    "babel-preset-react-app": "^9.0.0",
    "core-js-compat": "^3.1.3",
    "eslint-config-vacuumlabs": "^1.8.2",
    "eslint-import-resolver-alias": "^1.1.2",
    "extract-react-intl-messages": "^1.0.1",
    "flow-bin": "^0.92.1",
    "flow-coverage-report": "^0.6.1",
    "flow-typed": "^2.5.2",
    "graphql-code-generator": "^0.18.2",
    "graphql-codegen-flow": "^0.18.2",
    "graphql-codegen-flow-common": "^0.15.0-alpha.1885b0cc",
    "graphql-codegen-flow-documents": "^0.18.2",
    "node-sass": "^4.12.0",
    "prettier-eslint-cli": "^4.7.1",
    "sentry-testkit": "^2.2.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "flow-coverage-report": {
    "globIncludePatterns": [
      "src/**/*.js"
    ],
    "reportTypes": [
      "text",
      "html",
      "json"
    ]
  }
}
