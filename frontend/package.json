{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "SENTRY_COMMIT_HASH=`sentry-cli releases propose-version` react-app-rewired build",
    "build:heroku": "sentry-cli releases new $SOURCE_VERSION; REACT_APP_SENTRY_RELEASE_VERSION=$SOURCE_VERSION react-app-rewired build",
    "heroku-postbuild": "yarn build:heroku",
    "eject": "react-app-rewired eject",
    "eslint": "eslint src --max-warnings=0",
    "flow": "flow",
    "flow-start": "flow start",
    "flow-stop": "flow stop",
    "flow-coverage": "flow-coverage-report || exit 0",
    "prettier": "prettier-eslint --write \"src/**/*.js\"",
    "extract-translations": "NODE_ENV=development yarn run extract-react-intl-messages -o src/i18n/locales/ -l en --flat 'src/**/*.js'",
    "test": "yarn flow && yarn eslint",
    "update-schema-type": "yarn gql-gen"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/styles": "^3.0.0-alpha.10",
    "@nivo/line": "^0.52.1",
    "@react-google-maps/api": "^1.1.0",
    "@sentry/browser": "^5.1.1",
    "apollo-cache-inmemory": "^1.4.2",
    "apollo-client": "^2.4.12",
    "apollo-link-http": "^1.5.11",
    "bignumber.js": "^8.0.2",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.0.8",
    "customize-cra": "^0.2.12",
    "eslint-plugin-react-hooks": "^1.5.0",
    "font-awesome": "^4.7.0",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "idx": "^2.5.3",
    "isemail": "^3.2.0",
    "jdenticon": "^2.1.1",
    "moment-timezone": "^0.5.25",
    "qrcode.react": "^0.9.2",
    "query-string": "^6.3.0",
    "rc-slider": "^8.6.6",
    "react": "^16.8.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-apollo": "^2.4.1",
    "react-apollo-hooks": "^0.4.3",
    "react-dom": "^16.8.0",
    "react-intl": "^2.8.0",
    "react-markdown": "^4.0.8",
    "react-measure": "^2.2.6",
    "react-router": "^4.4.0-beta.7",
    "react-router-dom": "^4.4.0-beta.7",
    "react-scripts": "2.1.3",
    "recharts": "^1.5.0",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "smoothscroll-polyfill": "^0.4.4",
    "use-react-router": "^1.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-syntax-json-strings": "^7.2.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@sentry/webpack-plugin": "^1.6.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-react-intl-auto": "^1.6.1",
    "babel-preset-react-app": "^7.0.2",
    "eslint-config-vacuumlabs": "^1.7.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "extract-react-intl-messages": "^0.14.0",
    "flow-bin": "^0.92.1",
    "flow-coverage-report": "^0.6.1",
    "flow-typed": "^2.5.1",
    "graphql-code-generator": "^0.17.0",
    "graphql-codegen-flow": "^0.17.0",
    "graphql-codegen-flow-common": "^0.15.0-alpha.1885b0cc",
    "graphql-codegen-flow-documents": "^0.17.0",
    "node-sass": "^4.11.0",
    "prettier-eslint-cli": "^4.7.1",
    "react-app-rewired": "^2.1.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "flow-coverage-report": {
    "globIncludePatterns": [
      "src/**/*.js"
    ],
    "reportTypes": [
      "text",
      "html",
      "json"
    ]
  }
}
