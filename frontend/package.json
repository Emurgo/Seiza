{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "if test $NODE_ENV = production; then yarn start:heroku; else react-app-rewired start; fi",
    "start:heroku": "SENTRY_COMMIT_HASH=$HEROKU_SLUG_COMMIT node heroku.js",
    "build": "SENTRY_COMMIT_HASH=`sentry-cli releases propose-version` react-app-rewired build",
    "build:heroku": "sentry-cli releases new $SOURCE_VERSION; REACT_APP_SENTRY_RELEASE_VERSION=$SOURCE_VERSION react-app-rewired build --source-maps; sentry-cli releases files $SOURCE_VERSION upload-sourcemaps --ext map ./build",
    "heroku-postbuild": "yarn build:heroku",
    "eject": "react-app-rewired eject",
    "eslint": "eslint src --max-warnings=0",
    "flow": "flow",
    "flow-start": "flow start",
    "flow-stop": "flow stop",
    "flow-coverage": "flow-coverage-report || exit 0",
    "prettier": "prettier-eslint --write \"src/**/*.js\"",
    "extract-translations": "NODE_ENV=development src/i18n/scripts/cli.js --output src/i18n/locales/ --locales en,ja-JP,es-ES,ru-RU  'src/**/*.js' --flat",
    "test": "yarn flow && yarn eslint",
    "update-schema-type": "yarn gql-gen"
  },
  "dependencies": {
    "@material-ui/core": "^4.0.1",
    "@material-ui/icons": "^4.0.1",
    "@material-ui/styles": "^4.0.1",
    "@nivo/line": "^0.58.0",
    "@react-google-maps/api": "^1.2.3",
    "@sentry/browser": "^5.4.0",
    "apollo-cache-inmemory": "^1.6.0",
    "apollo-client": "^2.6.0",
    "apollo-link-http": "^1.5.14",
    "bignumber.js": "^9.0.0",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.2.0",
    "customize-cra": "^0.2.12",
    "eslint-plugin-react-hooks": "^1.6.0",
    "font-awesome": "^4.7.0",
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.1",
    "idx": "^2.5.6",
    "isemail": "^3.2.0",
    "jdenticon": "^2.1.1",
    "moment-timezone": "^0.5.25",
    "qrcode.react": "^0.9.2",
    "query-string": "^6.5.0",
    "rc-slider": "^8.6.11",
    "react": "^16.8.6",
    "react-addons-css-transition-group": "^15.6.2",
    "react-apollo": "^2.5.6",
    "react-apollo-hooks": "^0.4.5",
    "react-dom": "^16.8.6",
    "react-intl": "^2.9.0",
    "react-markdown": "^4.0.8",
    "react-measure": "^2.3.0",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1",
    "recharts": "^1.6.2",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "smoothscroll-polyfill": "^0.4.4",
    "unfetch": "^4.1.0",
    "use-react-router": "^1.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-syntax-json-strings": "^7.2.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@sentry/webpack-plugin": "^1.6.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-react-intl-auto": "^1.7.0",
    "babel-preset-react-app": "^9.0.0",
    "core-js-compat": "^3.1.3",
    "eslint-config-vacuumlabs": "^1.8.2",
    "eslint-import-resolver-alias": "^1.1.2",
    "extract-react-intl-messages": "^1.0.1",
    "flow-bin": "^0.92.1",
    "flow-coverage-report": "^0.6.1",
    "flow-typed": "^2.5.2",
    "graphql-code-generator": "^0.18.2",
    "graphql-codegen-flow": "^0.18.2",
    "graphql-codegen-flow-common": "^0.15.0-alpha.1885b0cc",
    "graphql-codegen-flow-documents": "^0.18.2",
    "node-sass": "^4.12.0",
    "prettier-eslint-cli": "^4.7.1",
    "react-app-rewired": "^2.1.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "flow-coverage-report": {
    "globIncludePatterns": [
      "src/**/*.js"
    ],
    "reportTypes": [
      "text",
      "html",
      "json"
    ]
  }
}
