{
  "name": "graphql-server",
  "version": "1.0.0",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "start": "babel-watch src/index.js -w src/graphql",
    "build": "rm -rf build && babel src --out-dir build",
    "serve": "node build/index.js",
    "eslint": "eslint src --max-warnings=0",
    "flow-stop": "flow stop",
    "flow": "flow",
    "flow-coverage": "flow-coverage-report || exit 0",
    "prettier": "prettier-eslint --write \"src/**/*.js\"",
    "jest": "jest",
    "test": "yarn flow && yarn eslint && yarn jest"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^0.1.0-beta.2",
    "@okgrow/graphql-scalars": "^0.4.5",
    "apollo-server": "^2.3.3",
    "axios": "^0.18.0",
    "axios-cache-adapter": "^2.3.0",
    "babel-plugin-import-graphql": "^2.6.2",
    "babel-polyfill": "^6.26.0",
    "bignumber.js": "^8.0.2",
    "cors": "^2.8.5",
    "flow-bin": "^0.92.1",
    "flow-coverage-report": "^0.6.1",
    "graphql": "^14.1.1",
    "graphql-bigint": "^1.0.0",
    "merge-graphql-schemas": "^1.5.8",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-watch": "^7.0.0",
    "eslint-config-vacuumlabs": "^1.7.0",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-react": "^7.12.4",
    "express": "^4.16.4",
    "jest": "^24.1.0",
    "prettier-eslint-cli": "^4.7.1"
  },
  "flow-coverage-report": {
    "globIncludePatterns": [
      "src/**/*.js"
    ],
    "reportTypes": [
      "text",
      "html",
      "json"
    ]
  }
}
